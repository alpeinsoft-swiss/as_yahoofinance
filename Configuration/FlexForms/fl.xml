<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>General</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.style>
                        <TCEforms>
                            <onChange>reload</onChange>
                            <label>LLL:EXT:as_yahoofinance/Configuration/FlexForms/locallang_db.xml:tt_content.as_yahoofinance.style</label>
                            <config>
                                <type>select</type>
                                <size>1</size>
                                <maxitems>1</maxitems>
                                <itemsProcFunc>asyahoofinance_itemsProcFunc->getStyle</itemsProcFunc>
                            </config>
                        </TCEforms>
                    </settings.style>

                    <settings.name>
                        <TCEforms>
                            <exclude>1</exclude>
                            <displayCond>FIELD:settings.style:=:chart</displayCond>
                            <label>LLL:EXT:as_yahoofinance/Configuration/FlexForms/locallang_db.xml:tt_content.as_yahoofinance.name</label>
                            <config>
                                <type>input</type>
                                <size>20</size>
                                <eval>required</eval>
                            </config>
                        </TCEforms>
                    </settings.name>

                    <settings.days>
                        <TCEforms>
                            <exclude>1</exclude>
                            <displayCond>FIELD:settings.style:=:chart</displayCond>
                            <label>LLL:EXT:as_yahoofinance/Configuration/FlexForms/locallang_db.xml:tt_content.as_yahoofinance.days</label>
                            <config>
                                <type>input</type>
                                <size>3</size>
                                <eval>required</eval>
                            </config>
                        </TCEforms>
                    </settings.days>

                    <settings.width>
                        <TCEforms>
                            <exclude>1</exclude>
                            <displayCond>FIELD:settings.style:=:chart</displayCond>
                            <label>LLL:EXT:as_yahoofinance/Configuration/FlexForms/locallang_db.xml:tt_content.as_yahoofinance.width</label>
                            <config>
                                <type>input</type>
                                <size>4</size>
                            </config>
                        </TCEforms>
                    </settings.width>

                    <settings.height>
                        <TCEforms>
                            <exclude>1</exclude>
                            <displayCond>FIELD:settings.style:=:chart</displayCond>
                            <label>LLL:EXT:as_yahoofinance/Configuration/FlexForms/locallang_db.xml:tt_content.as_yahoofinance.height</label>
                            <config>
                                <type>input</type>
                                <size>4</size>
                            </config>
                        </TCEforms>
                    </settings.height>

                    <settings.theme>
                        <TCEforms>
                            <exclude>1</exclude>
                            <displayCond>FIELD:settings.style:=:chart</displayCond>
                            <label>LLL:EXT:as_yahoofinance/Configuration/FlexForms/locallang_db.xml:tt_content.as_yahoofinance.chart_theme</label>
                            <config>
                                <type>select</type>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">LLL:EXT:as_yahoofinance/Configuration/FlexForms/locallang_db.xml:tt_content.as_yahoofinance.se_custom</numIndex>
                                        <numIndex index="1">LLL:EXT:as_yahoofinance/Configuration/FlexForms/locallang_db.xml:tt_content.as_yahoofinance.se_custom_t</numIndex>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">LLL:EXT:as_yahoofinance/Configuration/FlexForms/locallang_db.xml:tt_content.as_yahoofinance.se_light</numIndex>
                                        <numIndex index="1">LLL:EXT:as_yahoofinance/Configuration/FlexForms/locallang_db.xml:tt_content.as_yahoofinance.se_light_t</numIndex>
                                    </numIndex>
                                    <numIndex index="2" type="array">
                                        <numIndex index="0">LLL:EXT:as_yahoofinance/Configuration/FlexForms/locallang_db.xml:tt_content.as_yahoofinance.se_patterns</numIndex>
                                        <numIndex index="1">LLL:EXT:as_yahoofinance/Configuration/FlexForms/locallang_db.xml:tt_content.as_yahoofinance.se_patterns_t</numIndex>
                                    </numIndex>
                                    <numIndex index="3" type="array">
                                        <numIndex index="0">LLL:EXT:as_yahoofinance/Configuration/FlexForms/locallang_db.xml:tt_content.as_yahoofinance.se_dark</numIndex>
                                        <numIndex index="1">LLL:EXT:as_yahoofinance/Configuration/FlexForms/locallang_db.xml:tt_content.as_yahoofinance.se_dark_t</numIndex>
                                    </numIndex>
                                    <numIndex index="4" type="array">
                                        <numIndex index="0">LLL:EXT:as_yahoofinance/Configuration/FlexForms/locallang_db.xml:tt_content.as_yahoofinance.se_chalk</numIndex>
                                        <numIndex index="1">LLL:EXT:as_yahoofinance/Configuration/FlexForms/locallang_db.xml:tt_content.as_yahoofinance.se_chalk_t</numIndex>
                                    </numIndex>
                                </items>
                                <minitems>0</minitems>
                                <maxitems>1</maxitems>
                                <size>1</size>
                            </config>
                        </TCEforms>
                    </settings.theme>

                    <settings.positionY>
                        <TCEforms>
                            <exclude>1</exclude>
                            <displayCond>FIELD:settings.style:=:chart</displayCond>
                            <label>LLL:EXT:as_yahoofinance/Configuration/FlexForms/locallang_db.xml:tt_content.as_yahoofinance.position_ordinate</label>
                            <config>
                                <type>select</type>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0"></numIndex>
                                        <numIndex index="1"></numIndex>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">LLL:EXT:as_yahoofinance/Configuration/FlexForms/locallang_db.xml:tt_content.as_yahoofinance.po_left_t</numIndex>
                                        <numIndex index="1">LLL:EXT:as_yahoofinance/Configuration/FlexForms/locallang_db.xml:tt_content.as_yahoofinance.po_left_t</numIndex>
                                    </numIndex>
                                    <numIndex index="2" type="array">
                                        <numIndex index="0">LLL:EXT:as_yahoofinance/Configuration/FlexForms/locallang_db.xml:tt_content.as_yahoofinance.po_right_t</numIndex>
                                        <numIndex index="1">LLL:EXT:as_yahoofinance/Configuration/FlexForms/locallang_db.xml:tt_content.as_yahoofinance.po_right_t</numIndex>
                                    </numIndex>
                                </items>
                                <minitems>0</minitems>
                                <maxitems>1</maxitems>
                                <size>1</size>
                            </config>
                        </TCEforms>
                    </settings.positionY>

                    <settings.showScrollbar>
                        <TCEforms>
                            <exclude>1</exclude>
                            <displayCond>FIELD:settings.style:=:chart</displayCond>
                            <label>LLL:EXT:as_yahoofinance/Configuration/FlexForms/locallang_db.xml:tt_content.as_yahoofinance.show_scrollbar</label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </settings.showScrollbar>

                    <settings.fontSize>
                        <TCEforms>
                            <exclude>1</exclude>
                            <displayCond>FIELD:settings.style:=:chart</displayCond>
                            <label>LLL:EXT:as_yahoofinance/Configuration/FlexForms/locallang_db.xml:tt_content.as_yahoofinance.font_size</label>
                            <config>
                                <type>input</type>
                                <size>2</size>
                            </config>
                        </TCEforms>
                    </settings.fontSize>

                    <settings.fillColor>
                        <TCEforms>
                            <exclude>1</exclude>
                            <displayCond>FIELD:settings.style:=:chart</displayCond>
                            <label>LLL:EXT:as_yahoofinance/Configuration/FlexForms/locallang_db.xml:tt_content.as_yahoofinance.fill_color</label>
                            <config>
                                <type>input</type>
                                <size>6</size>
                                <wizards>
                                    <_PADDING>2</_PADDING>
                                    <color>
                                        <type>colorbox</type>
                                        <dim>12x12</dim>
                                        <tableStyle>border:solid 1px black;</tableStyle>
                                        <module type="array">
                                            <name>wizard_colorpicker</name>
                                            <urlParameters type="array">
                                                <mode>wizard</mode>
                                                <act>file</act>
                                            </urlParameters>
                                        </module>

                                        <JSopenParams>height=300,width=450,status=0,menubar=0,scrollbars=1
                                        </JSopenParams>
                                    </color>
                                </wizards>
                            </config>
                        </TCEforms>
                    </settings.fillColor>

                    <settings.fontColor>
                        <TCEforms>
                            <exclude>1</exclude>
                            <displayCond>FIELD:settings.style:=:chart</displayCond>
                            <label>LLL:EXT:as_yahoofinance/Configuration/FlexForms/locallang_db.xml:tt_content.as_yahoofinance.scale_font_color</label>
                            <config>
                                <type>input</type>
                                <size>6</size>
                                <wizards>
                                    <_PADDING>2</_PADDING>
                                    <color>
                                        <type>colorbox</type>
                                        <dim>12x12</dim>
                                        <tableStyle>border:solid 1px black;</tableStyle>
                                        <module type="array">
                                            <name>wizard_colorpicker</name>
                                            <urlParameters type="array">
                                                <mode>wizard</mode>
                                                <act>file</act>
                                            </urlParameters>
                                        </module>

                                        <JSopenParams>height=300,width=450,status=0,menubar=0,scrollbars=1
                                        </JSopenParams>
                                    </color>
                                </wizards>
                            </config>
                        </TCEforms>
                    </settings.fontColor>

                    <settings.scrollbarSelectedBackground>
                        <TCEforms>
                            <exclude>1</exclude>
                            <displayCond>FIELD:settings.style:=:chart</displayCond>
                            <label>LLL:EXT:as_yahoofinance/Configuration/FlexForms/locallang_db.xml:tt_content.as_yahoofinance.scrollbar_selected_background</label>
                            <config>
                                <type>input</type>
                                <size>6</size>
                                <wizards>
                                    <_PADDING>2</_PADDING>
                                    <color>
                                        <type>colorbox</type>
                                        <dim>12x12</dim>
                                        <tableStyle>border:solid 1px black;</tableStyle>
                                        <module type="array">
                                            <name>wizard_colorpicker</name>
                                            <urlParameters type="array">
                                                <mode>wizard</mode>
                                                <act>file</act>
                                            </urlParameters>
                                        </module>

                                        <JSopenParams>height=300,width=450,status=0,menubar=0,scrollbars=1
                                        </JSopenParams>
                                    </color>
                                </wizards>
                            </config>
                        </TCEforms>
                    </settings.scrollbarSelectedBackground>

                    <settings.cursorColor>
                        <TCEforms>
                            <exclude>1</exclude>
                            <displayCond>FIELD:settings.style:=:chart</displayCond>
                            <label>LLL:EXT:as_yahoofinance/Configuration/FlexForms/locallang_db.xml:tt_content.as_yahoofinance.cursor_color</label>
                            <config>
                                <type>input</type>
                                <size>6</size>
                                <wizards>
                                    <_PADDING>2</_PADDING>
                                    <color>
                                        <type>colorbox</type>
                                        <dim>12x12</dim>
                                        <tableStyle>border:solid 1px black;</tableStyle>
                                        <module type="array">
                                            <name>wizard_colorpicker</name>
                                            <urlParameters type="array">
                                                <mode>wizard</mode>
                                                <act>file</act>
                                            </urlParameters>
                                        </module>

                                        <JSopenParams>height=300,width=450,status=0,menubar=0,scrollbars=1
                                        </JSopenParams>
                                    </color>
                                </wizards>
                            </config>
                        </TCEforms>
                    </settings.cursorColor>

                    <settings.showTooltips>
                        <TCEforms>
                            <exclude>1</exclude>
                            <displayCond>FIELD:settings.style:=:chart</displayCond>
                            <label>LLL:EXT:as_yahoofinance/Configuration/FlexForms/locallang_db.xml:tt_content.as_yahoofinance.show_tooltips</label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </settings.showTooltips>

                    <!-- NOCHART -->

                    <settings.names>
                        <TCEforms>
                            <exclude>1</exclude>
                            <displayCond>FIELD:settings.style:=:nochart</displayCond>
                            <label>LLL:EXT:as_yahoofinance/Configuration/FlexForms/locallang_db.xml:tt_content.as_yahoofinance.names</label>
                            <config>
                                <type>input</type>
                                <size>20</size>
                                <eval>required</eval>
                            </config>
                        </TCEforms>
                    </settings.names>


                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>
